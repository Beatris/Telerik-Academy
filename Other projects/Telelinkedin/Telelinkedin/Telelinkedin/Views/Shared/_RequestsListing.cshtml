@model IEnumerable<Telelinkedin.ViewModels.ProfileViewModel>

<p class="lead">Requests</p>

@if (@Model.Count() == 0)
{
    <div>No Requests</div>
}
else
{
    foreach (var profile in @Model)
    {
        <span>@profile.Name</span>
        @Html.ActionLink("Connect", "Connect", new { id = profile.Id })
        <hr />
    }
}